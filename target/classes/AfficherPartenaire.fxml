<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.Controllers.AfficherPartenaire">
   <children>
      <Button fx:id="submitButton" layoutX="187.0" layoutY="330.0" mnemonicParsing="false" onAction="#handleSubmitButtonAction" prefHeight="26.0" prefWidth="211.0" text="Submit" textFill="#d60489" />
      <TableView fx:id="tableViewPartenaires" layoutX="75.0" layoutY="53.0" prefHeight="265.0" prefWidth="436.0">
         <columns>
            <TableColumn fx:id="columnNom" text="Nom">
               <cellValueFactory>
                  <PropertyValueFactory property="nom" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="columnMarque" text="Marque">
               <cellValueFactory>
                  <PropertyValueFactory property="marque" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="columnEmail" text="Email">
               <cellValueFactory>
                  <PropertyValueFactory property="email" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="columnDescription" text="Description">
               <cellValueFactory>
                  <PropertyValueFactory property="description" />
               </cellValueFactory>
            </TableColumn>
        </columns>
      </TableView>
   </children>
</AnchorPane>
